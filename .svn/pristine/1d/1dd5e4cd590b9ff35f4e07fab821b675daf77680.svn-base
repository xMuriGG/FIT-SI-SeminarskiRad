@using CZE.Data.Models;
@model List<Osoba>
@{
    ViewBag.Title = "Index";

}

<h2>Pocetna</h2>
<table class="table">
    <thead class="thead-inverse">
        <tr>
            <th>Ime</th>
            <th>Godine</th>          
        </tr>
    </thead>



    <tbody>
        @foreach (Osoba o in Model)
        {
            <tr>
                <td>@o.Ime</td>
                <td>@o.Godine</td>
             </tr>
        }
        </tbody>
</table>
